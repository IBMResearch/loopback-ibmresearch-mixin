{
  "name": "ModelcacheItem",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "ModelCache": {
      "disableDefault": true,
      "methods": ["getAllItems", "methodWithParam", "methodWith2Param"]
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false
    }
  }
}
